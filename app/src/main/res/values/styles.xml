<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.Material3.DayNight">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="elevation">0dp</item>
        <item name="android:navigationBarColor">@color/colorPrimaryDark</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorControlNormal">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="MBMaterialCardviewStyle" parent="Widget.MaterialComponents.CardView">
        <item name="cardBackgroundColor">@color/colorPrimaryLight</item>
        <item name="strokeColor">@color/colorPrimary</item>
        <item name="strokeWidth">1dp</item>
        <item name="cardElevation">0dp</item>
        <item name="cardCornerRadius">6dp</item>
    </style>

    <style name="MBMaterialCardviewStyleViolet" parent="@style/Widget.Material3.CardView.Elevated">
        <item name="cardElevation">0dp</item>
    </style>

    <style name="MBMaterialCardviewStyleFilledViolet" parent="MBMaterialCardviewStyleViolet">
        <item name="cardBackgroundColor">@color/colorPrimary</item>
        <item name="strokeColor">@color/colorPrimaryDark</item>
    </style>

    <style name="MBMaterialCardviewStyleYellow" parent="MBMaterialCardviewStyle">
        <item name="cardBackgroundColor">@color/colorPrimaryLight</item>
        <item name="strokeColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialCardviewStyleGrey" parent="MBMaterialCardviewStyle">
        <item name="cardBackgroundColor">@color/colorGrey</item>
        <item name="strokeColor">@color/colorGrey</item>
    </style>

    <style name="MBMaterialCardviewStyleGreen" parent="MBMaterialCardviewStyle">
        <item name="cardBackgroundColor">@color/colorPrimaryLight</item>
        <item name="strokeColor">@color/colorSuccess</item>
    </style>

    <style name="MBMaterialCardviewStyleRed" parent="MBMaterialCardviewStyle">
        <item name="cardBackgroundColor">@color/colorPrimaryLight</item>
        <item name="strokeColor">@color/colorError</item>
    </style>

    <style name="MBMaterialTextTitleThinViolet" parent="TextAppearance.Material3.LabelLarge">
        <item name="android:fontFamily">@font/yantramanav_bold</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textSize">@dimen/text_22</item>
    </style>

    <style name="MBMaterialTextTitleThinYellow" parent="MBMaterialTextTitleThinViolet">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="MBMaterialTextTitleViolet" parent="MBMaterialTextTitleThinViolet">
        <item name="android:fontFamily">@font/yantramanav_light</item>
    </style>

    <style name="MBMaterialTextTitleYellow" parent="MBMaterialTextTitleViolet">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="MBMaterialTextHeaderViolet" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/yantramanav_light</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/text_36</item>
    </style>

    <style name="MBMaterialTextHeaderYellow" parent="MBMaterialTextHeaderViolet">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="MBMaterialTextHeaderThinYellow" parent="MBMaterialTextHeaderYellow">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
    </style>

    <style name="MBMaterialTextHeaderThinViolet" parent="MBMaterialTextHeaderThinYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextHeaderBigYellow" parent="MBMaterialTextHeaderYellow">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
        <item name="android:textSize">@dimen/text_48</item>
    </style>

    <style name="MBMaterialTextHeaderBigViolet" parent="MBMaterialTextHeaderBigYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextHeaderBigThinViolet" parent="MBMaterialTextHeaderBigYellow">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">52sp</item>
    </style>

    <style name="MBMaterialTextHeaderBigThinWhite" parent="MBMaterialTextHeaderBigThinViolet">
        <item name="android:textColor">@color/colorPrimaryLight</item>
    </style>

    <style name="MBMaterialTextHeaderBigThinYellow" parent="MBMaterialTextHeaderBigThinViolet">
        <item name="android:textColor">@color/colorAccent</item>
    </style>


    <style name="MBMaterialTextDescrSmallViolet" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="MBMaterialTextDescrSmallYellow" parent="MBMaterialTextDescrSmallViolet">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="MBMaterialTextDescrSmallThinYellow" parent="MBMaterialTextDescrSmallYellow">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
    </style>

    <style name="MBMaterialTextDescrSmallThinWhite" parent="MBMaterialTextDescrSmallThinYellow">
        <item name="android:textColor">@color/colorPrimaryLight</item>
    </style>

    <style name="MBMaterialTextDescrSmallThinViolet" parent="MBMaterialTextDescrSmallThinYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextDescrSmallBoldYellow" parent="MBMaterialTextDescrSmallYellow">
        <item name="android:fontFamily">@font/yantramanav_light</item>
    </style>

    <style name="MBMaterialTextDescrSmallBoldViolet" parent="MBMaterialTextDescrSmallBoldYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextDescrUltraSmallBoldYellow" parent="MBMaterialTextDescrSmallBoldYellow">
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="MBMaterialTextDescrUltraSmallBoldViolet" parent="MBMaterialTextDescrSmallBoldViolet">
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="MBMaterialTextDescrUltraSmallYellow" parent="MBMaterialTextDescrSmallYellow">
        <item name="android:textSize">@dimen/text_14</item>
    </style>

    <style name="MBMaterialTextDescrSmallRed" parent="MBMaterialTextDescrSmallYellow">
        <item name="android:textColor">@color/colorError</item>
    </style>

    <style name="MBMaterialTextSubtitleSmallYellow" parent="MBMaterialTextSubtitleViolet">
        <item name="android:textSize">@dimen/text_18</item>
    </style>

    <style name="MBMaterialTextSubtitleSmallThinViolet" parent="MBMaterialTextSubtitleViolet">
        <item name="android:textSize">@dimen/text_22</item>
        <item name="android:fontFamily">@font/yantramanav_thin</item>
    </style>

    <style name="MBMaterialTextSubtitleSmallViolet" parent="MBMaterialTextSubtitleSmallYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextSubtitleYellow" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/yantramanav_light</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textSize">@dimen/text_26</item>
    </style>

    <style name="MBMaterialTextSubtitleViolet" parent="MBMaterialTextSubtitleYellow">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextSubtitleThinViolet" parent="MBMaterialTextSubtitleViolet">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="MBMaterialTextSubtitleThinYellow" parent="MBMaterialTextSubtitleYellow">
        <item name="android:fontFamily">@font/yantramanav_thin</item>
    </style>

    <style name="MBMaterialTextSubtitleThinWhite" parent="MBMaterialTextSubtitleThinYellow">
        <item name="android:textColor">@color/colorPrimaryLight</item>
    </style>

    <style name="MBMaterialChipDescrSmallThinWhite" parent="Widget.Material3.Chip.Assist">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textAppearance">@style/MBMaterialChipTextAppearance</item>
        <item name="chipBackgroundColor">?attr/colorSurfaceVariant</item>
        <item name="chipStrokeWidth">0dp</item>
    </style>

    <style name="MBMaterialChipDescrSmallThinViolet" parent="Widget.Material3.Chip.Assist">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:textAppearance">@style/MBMaterialChipTextAppearance</item>
        <item name="chipBackgroundColor">?attr/colorSurfaceVariant</item>
        <item name="chipStrokeWidth">0dp</item>
    </style>

    <style name="MBMaterialChipTextAppearance" parent="TextAppearance.Material3.LabelLarge">
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>

    <style name="MBMaterialButtonStyleYellow" parent="Widget.Material3.Button.ElevatedButton">
        <item name="backgroundTint">@android:color/background_light</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="strokeColor">@color/colorAccent</item>
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>

    <style name="MBMaterialButtonStyleRed" parent="MBMaterialButtonStyleYellow">
        <item name="backgroundTint">@color/colorLight</item>
        <item name="android:textColor">@color/colorError</item>
        <item name="strokeColor">@color/colorErrorSecondary</item>
    </style>

    <style name="MBMaterialButtonStyleViolet" parent="Widget.Material3.Button.ElevatedButton">
        <item name="backgroundTint">@color/colorPrimary</item>
        <item name="android:textColor">@color/colorLight</item>
        <item name="strokeColor">@color/colorPrimaryDark</item>
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>

    <style name="MBMaterialButtonStyleOutlinePurple" parent="Widget.Material3.Button.OutlinedButton">
        <item name="backgroundTint">@color/colorLight</item>
        <item name="android:textColor">@color/colorPurple</item>
        <item name="strokeColor">@color/colorPurple</item>
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>

    <style name="MBMaterialButtonStyleOutlineViolet" parent="Widget.Material3.Button.OutlinedButton">
        <item name="backgroundTint">@color/colorLight</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="strokeColor">@color/colorPrimary</item>
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>

    <style name="MBMaterialButtonStyleOutlineYellow" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="strokeColor">@color/colorAccent</item>
        <item name="fontFamily">@font/yantramanav_bold</item>
    </style>


    <style name="MBMaterialTextInputOutlinedYellow" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/colorAccent</item>
        <item name="boxStrokeWidth">2dp</item>
    </style>

    <style name="MBMaterialTextInputOutlinedViolet" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/colorPrimary</item>
        <item name="boxStrokeWidth">2dp</item>
    </style>
</resources>
